generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x", "linux-arm64-openssl-1.1.x", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id                String    @id @default(cuid())
  name              String?
  email             String?
  password          String?
  contactNo         String?   
  address           String?
  state             String?
  zipCode           String?
  countryId         Int?
  country           String?
  stateId           Int?
  phoneCode         String?   @db.VarChar(8)
  language          String?
  companyName       String?
  roleId            String?      @default("3")
  roleName          String?      @default("customer")
  profileStatus     String?
  image             String?
  isDeleted         String?      @default("N")
  createdBy         String?
  packageId         String?  
  packageName       String?  
  packageValidity   String?
  purchaseDate      DateTime  @default(now())
  expirayDate       DateTime?
  paymentMethod     String?
  isExpired         String?
  amount            String?  
  currency          String?  
  paymentStatus     String?  
  paymentId         String?
  adminApprovalId   String?    @default("3")
  deviceToken       String?
  createdAt         DateTime           @default(now())
  updatedAt         DateTime           @updatedAt
}

model verification_token {
  identifier String
  token      String   @unique(map: "VerificationToken_token_key")
  expires    DateTime

  @@unique([identifier, token], map: "VerificationToken_identifier_token_key")
}

model role {
    id              String @id @default(cuid())
